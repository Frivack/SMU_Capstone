<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:clipChildren="false"
        android:clipToPadding="false"
        android:id="@+id/rfikhe3uf1u"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/background"
        android:paddingTop="9dp"
        android:elevation="4dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ryyrxnk48qf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:layout_marginHorizontal="6dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/rp4jzsdf7i8m"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:layout_marginEnd="34dp"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/img_logo"
                        android:scaleType="fitXY"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rx1zz8d7691c"
                    android:layout_weight="1"
                    android:layout_width="80dp"
                    android:layout_height="24dp"
                    android:layout_marginEnd="40dp"
                    android:layout_marginStart="40dp"
                    android:layout_gravity="end"
                    android:background="@drawable/layout_background">
                    <TextView
                        android:id="@+id/mainpage_id"
                        android:textColor="#411A66"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:layout_marginStart="5dp"
                        android:layout_gravity="center_vertical"
                        android:text="@string/id"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rfh0v5aa39v9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="31dp"
                android:layout_marginHorizontal="40dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/r9erq9d2ouq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/layout_rectangle"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/r1d7rdjqb2bj"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/img_whatido85"
                        android:scaleType="fitXY"
                        android:onClick="onClickMainToBuild"
                        />
                    <TextView
                        android:id="@+id/rea8v3wm5z8e"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="신규/수정 견적"
                        />
                </LinearLayout>
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <LinearLayout
                    android:id="@+id/r5bgxagn6cgp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/layout_rectangle"
                    android:elevation="4dp"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/r1d7rsd1bj"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/img_account"
                        android:scaleType="fitXY"
                        android:onClick="onClickMainToBuild"
                        />
                    <TextView
                        android:id="@+id/r8t7zo8z5auc"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="계정"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rkrybo4jfbcd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="25dp"
                android:layout_marginHorizontal="40dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/rzic1x5yyhc"
                    android:layout_width="121dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_rectangle"
                    android:elevation="4dp"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:id="@+id/v1arsd1bj"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/img_account"
                        android:scaleType="fitXY"
                        android:onClick="onClickMainToBuild"
                        />
                    <TextView
                        android:id="@+id/deving2"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="리뷰 보기"
                        />
                </LinearLayout>
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <LinearLayout
                    android:id="@+id/rnfhr7ld490n"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/layout_rectangle"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/rijdf8am3vt"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/img_automatic85"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/rs6556pyx5tq"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="환경설정"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rx5ekyvmao7e"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="33dp"
                android:layout_marginHorizontal="40dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/rphm3k8j1duo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_rectangle"
                    android:orientation="vertical"
                    android:gravity="center">
                    <ImageView
                        android:id="@+id/sdg43azv1k"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/img_support85"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/deving3"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="고객센터"
                        />
                </LinearLayout>
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <LinearLayout
                    android:id="@+id/rwnlyf0kldxh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="center_horizontal"
                    android:background="@drawable/layout_rectangle"
                    android:elevation="4dp"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/rb9mjbueceg8"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/img_event85"
                        android:scaleType="fitXY"
                        android:onClick="onClickMainToSetting"
                        />
                    <TextView
                        android:id="@+id/rnwd0rymnwfd"
                        android:textColor="#000000"
                        android:textSize="15sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="공지사항"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rk4h9uxmdwns"
                android:layout_width="296dp"
                android:layout_height="42dp"
                android:background="@drawable/layout_whiteshadow"
                android:layout_marginBottom="48dp"
                android:orientation="vertical"
                android:layout_gravity="center"
                android:gravity="center">
                <TextView
                    android:id="@+id/tocomputer"
                    android:textColor="#411A66"
                    android:textSize="20sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="나만의 컴퓨터 만들러 가기"
                    android:onClick="onClickMainToBuild"
                    />
            </LinearLayout>

            <LinearLayout android:id="@+id/linearRowhomeOne"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:background="@drawable/background"
                android:layout_marginHorizontal="16dp"
                android:orientation="horizontal">
                <ImageView android:id="@+id/imageHomeOne"
                    android:layout_gravity="center"
                    android:layout_height="40dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <View android:background="#30000000"
                    android:id="@+id/lineLineoneOne"
                    android:layout_height="59dp" android:layout_width="1dp"/>
                <ImageView android:id="@+id/imageWhatidoOne"
                    android:layout_gravity="center"
                    android:layout_height="40dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:src="@drawable/img_what_i_do"/>
                <View android:background="#30000000"
                    android:id="@+id/lineLineoneThree"
                    android:layout_height="59dp" android:layout_width="1dp"/>
                <ImageView android:id="@+id/imageAutomaticOne"
                    android:layout_gravity="center"
                    android:layout_height="40dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:src="@drawable/img_automatic"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_automatic"/>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>
